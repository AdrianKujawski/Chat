<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="262.0" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.FXMLDocumentController">
    <children>
        <Button fx:id="sendButton" layoutX="255.0" layoutY="225.0" onAction="#sendToServer" text="Wyślij" />
      <TextArea fx:id="textArea" layoutX="16.0" layoutY="49.0" prefHeight="139.0" prefWidth="288.0" wrapText="true" />
      <Button fx:id="connectButton" layoutX="185.0" layoutY="14.0" mnemonicParsing="false" onAction="#connectToServer" text="Połącz" />
      <TextField fx:id="yourName" layoutX="14.0" layoutY="14.0" onAction="#onEnterConnect" />
      <Button fx:id="disconnectButton" layoutX="250.0" layoutY="14.0" mnemonicParsing="false" onAction="#disconnectFromServer" text="Rozłącz" />
      <TextArea fx:id="textField" accessibleRole="TEXT_FIELD" layoutX="16.0" layoutY="197.0" onKeyPressed="#onEnterSend" prefHeight="56.0" prefWidth="218.0" wrapText="true" />
    </children>
</AnchorPane>
